@model Duel


<body>
    <h1>@Model.User1 vs @Model.User2</h1>
    <h3>Trivia Question!</h3>
    <p>True or False?</p>
    @foreach (var forecast in Model.Trivia)
    {
        <div>
            <div class="form-group">
                <h4>Category:</h4>
                <div>@forecast.category</div>
            </div>
            <div class="form-group">
                <h4>Question:</h4>
                <h4>@forecast.question</h4>
            </div>
        </div>
    }
    <partial name="AnswerForm">
</body>

</html>


